{
  "version": 1,
  "description": "Color palettes based on Open Colors (Excalidraw's native palette)",
  "palettes": {
    "architecture": {
      "name": "Architecture",
      "description": "System and service architecture diagrams",
      "colors": {
        "primary": { "fill": "#a5d8ff", "stroke": "#1971c2", "label": "Primary/UI" },
        "control": { "fill": "#b2f2bb", "stroke": "#2f9e44", "label": "Control plane" },
        "agent": { "fill": "#ffd8a8", "stroke": "#e8590c", "label": "Agents/External" },
        "workflow": { "fill": "#ffc9c9", "stroke": "#e03131", "label": "Workflows" },
        "ai": { "fill": "#eebefa", "stroke": "#9c36b5", "label": "AI/ML services" },
        "data": { "fill": "#96f2d7", "stroke": "#087f5b", "label": "Data stores" },
        "infra": { "fill": "#dee2e6", "stroke": "#495057", "label": "Infrastructure" },
        "text": "#1e1e1e",
        "background": "#ffffff"
      }
    },
    "workflow": {
      "name": "Workflow",
      "description": "State machines and process flows",
      "colors": {
        "pending": { "fill": "#ffec99", "stroke": "#e67700", "label": "Pending/Waiting" },
        "in_progress": { "fill": "#ffd8a8", "stroke": "#e8590c", "label": "In Progress" },
        "success": { "fill": "#b2f2bb", "stroke": "#2f9e44", "label": "Success/Complete" },
        "error": { "fill": "#ffc9c9", "stroke": "#e03131", "label": "Error/Failed" },
        "cancelled": { "fill": "#dee2e6", "stroke": "#495057", "label": "Cancelled/Skipped" },
        "text": "#1e1e1e",
        "background": "#ffffff"
      }
    },
    "aws": {
      "name": "AWS",
      "description": "AWS cloud architecture diagrams",
      "colors": {
        "compute": { "fill": "#ffd8a8", "stroke": "#d9480f", "label": "Compute (EC2, Lambda)" },
        "storage": { "fill": "#b2f2bb", "stroke": "#2f9e44", "label": "Storage (S3, EBS)" },
        "database": { "fill": "#a5d8ff", "stroke": "#1971c2", "label": "Database (RDS, DynamoDB)" },
        "network": { "fill": "#eebefa", "stroke": "#9c36b5", "label": "Network (VPC, ELB)" },
        "security": { "fill": "#ffc9c9", "stroke": "#e03131", "label": "Security (IAM, KMS)" },
        "integration": { "fill": "#ffec99", "stroke": "#e67700", "label": "Integration (SQS, SNS)" },
        "text": "#1e1e1e",
        "background": "#ffffff"
      }
    },
    "k8s": {
      "name": "Kubernetes",
      "description": "Kubernetes deployment diagrams",
      "colors": {
        "pod": { "fill": "#a5d8ff", "stroke": "#1864ab", "label": "Pods" },
        "service": { "fill": "#b2f2bb", "stroke": "#2f9e44", "label": "Services" },
        "ingress": { "fill": "#eebefa", "stroke": "#9c36b5", "label": "Ingress/Gateway" },
        "storage": { "fill": "#ffd8a8", "stroke": "#e8590c", "label": "Storage (PV, PVC)" },
        "config": { "fill": "#ffec99", "stroke": "#e67700", "label": "Config (CM, Secret)" },
        "namespace": { "fill": "#dee2e6", "stroke": "#495057", "label": "Namespace boundary" },
        "text": "#1e1e1e",
        "background": "#ffffff"
      }
    },
    "minimal": {
      "name": "Minimal",
      "description": "Clean, minimal diagrams with single accent",
      "colors": {
        "primary": { "fill": "#dee2e6", "stroke": "#495057", "label": "Primary elements" },
        "accent": { "fill": "#a5d8ff", "stroke": "#1971c2", "label": "Accent/Highlight" },
        "text": "#1e1e1e",
        "background": "#ffffff"
      }
    }
  },
  "openColors": {
